{
    "private": true,
    "scripts": {
        "dev": "npm run dev.js && npm run dev.css",
        "watch": "concurrently \"npm run watch.js\" \"npm run watch.css\"",
        "watch-all": "concurrently \"npm run watch.js\" \"npm run watch.css\" \"npm run watch-fe\"",
        "prod-all": "npm run prod-fe && npm run prod",
        "prod": "npm run prod.js && npm run prod.css",
        "report": "npm run prod.js.report",
        "dev-fe": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack-frontend.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch-fe": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --watch --hide-modules --env.mixfile=webpack-frontend.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod-fe": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack-frontend.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch.css": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --watch --hide-modules --env.mixfile=webpack-css.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "dev.css": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack-css.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod.css": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack-css.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch.js": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --watch --hide-modules --env.mixfile=webpack-js.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "dev.js": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack-js.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod.js": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack-js.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod.js.report": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --report --env.mixfile=webpack-js.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "eslint:fix": "eslint resources/js/**/*.{js,vue} --fix",
        "prettier:fix": "prettier --write resources/js/**/*.{js,vue}",
        "prettier:php": "prettier --write --parser=php"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "resources/**/*.{js,vue}": [
            "eslint --fix",
            "prettier --write"
        ],
        "resources/**/*.{sass,scss}": [
            "prettier --write"
        ],
        "*.php": [
            "prettier --write --paser=php"
        ]
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@prettier/plugin-php": "^0.15.0",
        "autoprefixer": "^9.8.6",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "browser-sync": "^2.26.12",
        "browser-sync-webpack-plugin": "^2.2.2",
        "compression-webpack-plugin": "^6.0.3",
        "concurrently": "^5.3.0",
        "core-js": "^3.6.5",
        "cross-env": "^7.0.2",
        "css-loader": "^3.6.0",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-eslint-plugin": "^2.3.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-vue": "^7.0.1",
        "husky": "^4.3.0",
        "jquery": "^3.5.1",
        "laravel-mix": "^5.0.7",
        "laravel-mix-eslint": "^0.1.3",
        "laravel-mix-purgecss": "^5.0.0",
        "lint-staged": "^10.4.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.1.2",
        "purgecss-whitelister": "^2.4.0",
        "resolve-url-loader": "^3.1.1",
        "rtlcss": "^2.6.0",
        "sass": "^1.27.0",
        "sass-loader": "^10.0.3",
        "script-loader": "^0.7.2",
        "svg-sprite-loader": "^5.0.0",
        "vue-template-compiler": "^2.6.12",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-chunk-rename-plugin": "^1.0.3",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "axios": "^0.20.0",
        "codemirror": "^5.58.1",
        "dropzone": "^5.7.2",
        "echarts": "^4.9.0",
        "element-ui": "^2.13.2",
        "file-saver": "^2.0.2",
        "js-cookie": "^2.2.1",
        "jsonlint": "^1.6.3",
        "jszip": "^3.5.0",
        "lodash": "^4.17.20",
        "moment": "^2.29.1",
        "nprogress": "^0.2.0",
        "path-to-regexp": "^6.2.0",
        "tailwindcss": "^1.9.1",
        "tui-editor": "^1.4.10",
        "vue": "^2.6.12",
        "vue-count-to": "^1.0.13",
        "vue-i18n": "^8.22.0",
        "vue-router": "^3.4.6",
        "vue2-dropzone": "^3.6.0",
        "vuedraggable": "^2.24.2",
        "vuex": "^3.5.1",
        "xlsx": "^0.16.8"
    }
}
