        "prod.js": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack-js.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod.js.report": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --report --env.mixfile=webpack-js.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "eslint:fix": "eslint resources/js/**/*.{js,vue} --fix",
        "prettier:fix": "prettier --write resources/js/**/*.{js,vue}",
        "prettier:php": "prettier --write --parser=php"
