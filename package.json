{
    "private": true,
    "scripts": {
        "dev": "npm run dev.js & npm run dev.css",
        "watch": "npm run watch.js & npm run watch.css",
        "prod": "npm run prod.js & npm run prod.css",
        "dev-fe": "mix --mix-config=webpack-frontend.mix.js",
        "watch-fe": "mix watch --mix-config=webpack-frontend.mix.js",
        "prod-fe": "mix --production --mix-config=webpack-frontend.mix.js",
        "watch-all": "npm run watch & npm run watch-fe",
        "prod-all": "concurrently \"npm run prod-fe\" \"npm run prod\"",
        "dev.css": "mix --mix-config=webpack-css.mix.js",
        "watch.css": "mix watch --mix-config=webpack-css.mix.js",
        "prod.css": "mix --production --mix-config=webpack-css.mix.js",
        "dev.js": "mix --mix-config=webpack-js.mix.js",
        "watch.js": "mix watch --mix-config=webpack-js.mix.js",
        "prod.js": "mix --production --mix-config=webpack-js.mix.js",
        "dev.js.report": "mix --mix-config=webpack-js.mix.js -- --env --report",
        "prod.js.report": "mix --production --mix-config=webpack-js.mix.js -- --env --report",
        "dev-fe.report": "mix --report --mix-config=webpack-frontend.mix.js",
        "prod-fe.report": "mix --production --report --mix-config=webpack-frontend.mix.js",
        "eslint:fix": "eslint resources/js/**/*.{js,vue} --fix",
        "prettier:fix": "prettier --write resources/js/**/*.{js,vue}",
        "prettier:php": "prettier --write --parser=php"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "resources/**/*.{js,vue}": [
            "eslint --fix",
            "prettier --write"
        ],
        "resources/**/*.{sass,scss}": [
            "prettier --write"
        ],
        "*.php": [
            "prettier --write --paser=php"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
        "@vue/babel-plugin-transform-vue-jsx": "^1.2.1",
        "@prettier/plugin-php": "^0.16.1",
        "autoprefixer": "^10.2.5",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^7.1.2",
        "concurrently": "^6.0.0",
        "core-js": "^3.9.1",
        "cross-env": "^7.0.3",
        "css-loader": "^3.6.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-eslint-plugin": "^2.3.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-vue": "^7.7.0",
        "eslint-webpack-plugin": "^2.5.2",
        "husky": "^5.1.3",
        "laravel-mix": "^6.0.13",
        "laravel-mix-eslint": "^0.2.0",
        "laravel-mix-merge-manifest": "^2.0.0",
        "laravel-mix-purgecss": "^6.0.0",
        "lint-staged": "^10.5.4",
        "moment-locales-webpack-plugin": "^1.2.0",
        "node-sass": "^5.0.0",
        "path-browserify": "^1.0.1",
        "postcss": "^8.2.8",
        "postcss-rtl": "^1.7.3",
        "prettier": "^2.2.1",
        "purgecss-whitelister": "^2.4.0",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "script-loader": "^0.7.2",
        "style-loader": "^2.0.0",
        "svg-sprite-loader": "^6.0.2",
        "tailwindcss": "^2.0.4",
        "vue-loader": "^15.9.6",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.26.3",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-chunk-rename-plugin": "^1.0.3",
        "webpack-cli": "^4.5.0"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "codemirror": "^5.59.4",
        "dropzone": "^5.8.1",
        "echarts": "^5.0.2",
        "element-ui": "^2.15.1",
        "file-saver": "^2.0.5",
        "js-cookie": "^2.2.1",
        "jsonlint": "^1.6.3",
        "jszip": "^3.6.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "nprogress": "^0.2.0",
        "path-to-regexp": "^6.2.0",
        "tui-editor": "^1.4.10",
        "vue": "^2.6.12",
        "vue-count-to": "^1.0.13",
        "vue-i18n": "^8.24.1",
        "vue-router": "^3.5.1",
        "vue2-dropzone": "^3.6.0",
        "vuedraggable": "^2.24.3",
        "vuex": "^3.6.2",
        "xlsx": "^0.16.9"
    }
}
