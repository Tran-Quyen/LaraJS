        "resources/**/*.{js,vue}": [
            "eslint --fix",
            "prettier --write"
        ],
        "resources/**/*.{sass,scss}": [
            "prettier --write"
        ],
        "*.php": [
            "prettier --write --paser=php"
        ]
    },
